<div id="main-content" class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <div class="row">
                <div class="col-md-12" id="config-panel">
                  {{form-config  generate="generate" clear="clear" data=model}}
                </div>
            </div>
            <div class="row">
                <div id="code" class="col-md-12">
                  {{gen-output data=model }}
                </div>
            </div>
        </div>
        <div class="col-md-6" id="map">
            {{leaflet-map lat=0 lng=0 zoom=5 bounds=model.bounds markers=model.mapOptions.markers}}
        </div>
    </div>
</div>